<%= form_with(model: project, local: true) do |f| %>
  <ul>
    <% project.errors.full_messages.each do |msg| %>
      <li> <%= msg %> </li>
    <% end %>
  </ul>

    <div class = 'form-group' >
      <%= f.label :name %>
      <%= f.text_field :name ,class: "form-control"%>
    </div>
    <div class = 'form-group' >
      <%= f.label :description %>
      <%= f.text_area :description ,class: "form-control"%>
    </div>
    <%= f.submit %>
<% end %>
